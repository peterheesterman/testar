{
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
      "node": true,
      "jest": true
  },
  "rules": {
      "semi": 0,
      "no-unused-vars": 1,    
      "no-undef": "error",
      "no-undef-init": "error"
  }
}